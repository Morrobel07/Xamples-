@model IEnumerable<int>

@{
    Layout = null;
}

<!DOCTYPE html>
<html>

<head>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>Números Pares</title>
</head>

<body>
    <h2>Números Pares hasta el límite</h2>

    <ul>
        @foreach (var num in Model)
        {
            <li>@num</li>
        }
    </ul>
</body>

</html>